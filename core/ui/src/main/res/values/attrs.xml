<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DashboardView">
        <attr name="type" format="enum">
            <enum name="next" value="0" />
            <enum name="latest" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="VehicleView">
        <attr name="buttonText" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="LabelValueView">
        <attr name="label" format="string|reference" />
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr name="message" format="string|reference" />
        <attr name="primaryActionLabel" format="string|reference" />
        <attr name="secondaryActionLabel" format="string|reference" />
    </declare-styleable>

</resources>